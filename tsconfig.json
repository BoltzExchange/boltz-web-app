{
    "compilerOptions": {
        "noEmit": true,
        "target": "ESNext",
        "module": "ESNext",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "moduleResolution": "node",
        "jsx": "preserve",
        "jsxImportSource": "solid-js",
        "types": ["vite/client", "bignumber.js", "vitest/globals", "node"],
        "baseUrl": ".",
        "outDir": "ts-out",
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "skipLibCheck": true
    }
}
